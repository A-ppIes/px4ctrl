            <rosparam command="load" file="$(find px4ctrl)/config/ctrl_param_fpv.yaml" />
        </node>
    </group>
    <group ns="iris_2">
        <node pkg="px4ctrl" type="px4ctrl_node" name="px4ctrl_2" output="screen">
